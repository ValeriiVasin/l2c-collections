{
  "name": "l2c-collections",
  "version": "1.0.0",
  "description": "Lineage 2 Classic collections",
  "main": "index.js",
  "private": true,
  "scripts": {
    "lint": "eslint '{src,bin,cypress}/**/*.{ts,tsx,js,jsx,json,eslintrc,prettierrc}'",
    "check-types": "tsc --noEmit",
    "prepare": "husky install",
    "start": "react-scripts start",
    "start:ci": "serve -s build -l 3000",
    "build": "react-scripts build",
    "test": "jest src bin",
    "test:integration": "cypress run",
    "test:integration:ci": "start-server-and-test start:ci 3000 test:integration",
    "eject": "react-scripts eject"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/image-to-base64": "^2.1.0",
    "@types/jest": "^27.5.1",
    "@types/jsdom": "^16.2.14",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.27.0",
    "classnames": "^2.3.1",
    "cypress": "^9.7.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fuzzysort": "^2.0.1",
    "husky": "^8.0.1",
    "image-to-base64": "^2.2.0",
    "jest": "^28.0.0",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.4.2",
    "lodash": "^4.17.21",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "reset-css": "^5.0.1",
    "sass": "^1.52.1",
    "serve": "^13.0.2",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.7.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,scss,md,eslintrc,prettierrc,html}": [
      "prettier --write --ignore-unknown"
    ]
  }
}
